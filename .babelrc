{
  "presets": [
    [
      "@babel/env",
      {
        "modules": false,
        "targets": {
          "browsers": "> 1%"
        },
        "forceAllTransforms": true,
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react"
  ],
  "env": {
    "test": {
      "presets": ["@babel/env", "@babel/react"],
      "plugins": [
        "syntax-dynamic-import",
        "transform-object-rest-spread",
        [
          "@babel/proposal-class-properties",
          { "spec": true }
        ]
      ]
    }
  },
  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    [
      "@babel/proposal-class-properties",
      {
        "spec": true
      }
    ]
  ]
}
